{
	"name": "focus-full",
	"version": "0.1",
	"description": "A productivity tool to add to your toolbox",
	"permissions": [
		"activeTab",
		"storage",
		"declarativeContent",
		"<all_urls>"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"css": [
				"/static/css/new-style.css"
			],
			"js": [
				"/static/js/content-scripts.js"
			]
		}
	],
	"background": {
		"scripts": [
			"/static/js/background.js"
		],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {
			"16": "/imgs/default16.png",
			"32": "/imgs/default32.png",
			"48": "/imgs/default48.png",
			"64": "/imgs/default64.png"
		}
	},
	"icons": {
		"16": "/imgs/default16.png",
		"32": "/imgs/default32.png",
		"48": "/imgs/default48.png",
		"64": "/imgs/default64.png"
	},
	"commands": {
		"initiate-modular-search": {
			"suggested_key": {
				"default": "Ctrl+Shift+Q"
			},
			"description": "Ctrl+Shift+Q"
		}
	},
	"web_accesible_resources": [
		"imgs/blank.png"
	],


	"manifest_version": 2
}